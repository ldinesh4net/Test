<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>Freelancer - Start Bootstrap Theme</title>

    <!-- Bootstrap Core CSS -->
    <link href="~/UIScripts/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Theme CSS -->
    <link href="~/UIScripts/css/freelancer.min.css" rel="stylesheet">

    <!-- Custom Fonts -->
    <link href="~/UIScripts/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Lato:400,700,400italic,700italic" rel="stylesheet" type="text/css">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>




<body id="page-top" class="index">
    <!-- Wrapper-->
    <!-- PageClass give you ability to specify custom style for specific view based on action -->
    @Html.Partial("_TopNavbar")



   
    
            @RenderBody()
         


        @Html.Partial("_footer")

        <!-- Scroll to Top Button (Only visible on small and extra-small screen sizes) -->
        <div class="scroll-top page-scroll hidden-sm hidden-xs hidden-lg hidden-md">
            <a class="btn btn-primary" href="#page-top">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>

    <div class="ui small modal" id="CustomAlert" style="height:200px">
        <div class="header">
            Information
        </div>
        <div class="content">
            <div class="description">
                Please select the row
            </div>
        </div>
        <div class="actions">
        </div>
    </div>

        <!-- jQuery -->
        <script src="~/UIScripts/vendor/jquery/jquery.min.js"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="~/UIScripts/vendor/bootstrap/js/bootstrap.min.js"></script>

        <!-- Plugin JavaScript -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>

        <!-- Contact Form JavaScript -->
        <script src="~/UIScripts/js/jqBootstrapValidation.js"></script>
        <script src="~/UIScripts/js/contact_me.js"></script>

        <!-- Theme JavaScript -->
        <script src="~/UIScripts/js/freelancer.min.js"></script>
    @Scripts.Render("~/app/CustomAlert")

        @RenderSection("scripts", required: false)




</body>
</html>

